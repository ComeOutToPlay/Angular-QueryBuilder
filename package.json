{
  "name": "angular2-query-builder-14",
  "version": "5",
  "description": "A modernized Angular 2+ query builder based on jquery QueryBuilder",
  "main": "./dist/angular2-query-builder-14/bundles/angular2-query-builder-14.umd.js",
  "module": "./dist/angular2-query-builder-14/esm5/angular2-query-builder-14.js",
  "typings": "./dist/angular2-query-builder-14/angular2-query-builder-14.d.ts",
  "license": "MIT",
  "private": false,
  "keywords": [
    "angular",
    "angular8",
    "query",
    "builder",
    "jquery",
    "querybuilder",
    "visual"
  ],
  "author": {
    "name": "Zeb Zhao",
    "url": "https://github.com/zebzhao/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zebzhao/Angular-QueryBuilder"
  },
  "bugs": {
    "url": "https://github.com/zebzhao/Angular-QueryBuilder/issues"
  },
  "homepage": "https://github.com/zebzhao/Angular-QueryBuilder#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular2-query-builder-14",
    "build:watch": "ng build angular2-query-builder-14 --watch=true",
    "test": "ng test --watch=false",
    "e2e": "ng e2e",
    "ngcompile": "npm run build",
    "build:esm:watch": "npm build:watch",
    "build:umd": "npm run build",
    "build:umd:watch": "npm build:watch",
    "ci": "npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage",
    "explorer": "source-map-explorer ./dist/angular2-query-builder-14/bundles/angular2-query-builder-14.umd.js",
    "copyDemo": "copyfiles -u 2 demo/dist/**/* docs",
    "gh-pages": "rimraf docs && npm run docs && npm run copyDemo && gh-pages -d docs",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test:watch": "ng test --watch=true"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/language-service": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "^3.10.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.4.1",
    "core-js": "^3.29.1",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^3.2.3",
    "jasmine-core": "^3.10.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.0.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.8.7",
    "protractor": "^7.0.0",
    "rxjs": "~7.8.0",
    "ts-node": "^10.0.0",
    "typescript": "4.8.4",
    "zone.js": "~0.11.4"
  },
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/material": "^14.2.0",
    "@angular-eslint/schematics": "^1.0.0"
  }
}
