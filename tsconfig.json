{
  "compileOnSave": false,
  "compilerOptions": {

    "strict": false,
    "outDir": "../../out-tsc/lib",
    "declaration": true,
    "inlineSources": true,
    "types": [],
    "target": "es2020",
    "module": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [
      "es2020",
      "dom"
    ],

    "baseUrl": "./",
    "downlevelIteration": false,
    "importHelpers": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
