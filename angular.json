{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular2-query-builder-14": {
      "projectType": "library",
      "root": "projects/angular2-query-builder-14",
      "sourceRoot": "projects/angular2-query-builder-14/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/angular2-query-builder-14/tsconfig.lib.json",
            "project": "projects/angular2-query-builder-14/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular2-query-builder-14/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular2-query-builder-14/src/test.ts",
            "tsConfig": "projects/angular2-query-builder-14/tsconfig.spec.json",
            "karmaConfig": "projects/angular2-query-builder-14/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/angular2-query-builder-14/src/**/*.ts",
              "projects/angular2-query-builder-14/src/**/*.component.html"
            ]
          }
        }
      }
    }
  }
}
